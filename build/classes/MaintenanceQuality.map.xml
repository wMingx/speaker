<mapper namespace="maintenanceQuality">

	
<select id="selectMaintenanceQuality" parameterType="MaintenanceQuality" resultType="MaintenanceQuality">
		select  quality as quality, 							
				convert(nvarchar(MAX),intflag) as intflag 
		from maintenanceQuality_table 		
			</select>


