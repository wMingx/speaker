<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="maintenanceReason">

	
<select id="selectMaintenanceReason" parameterType="MaintenanceReason" resultType="MaintenanceReason">
		select  reason as reason, 							
				convert(nvarchar(MAX),intflag) as intflag 
		from maintenanceReason_table 		
			</select>

</mapper>